# Plex
PLEX_CLAIM=your_plex_claim_token_here

# User and group ID for permissions
PUID=1000
PGID=1000

# qBittorrent
QBT_USER=admin
QBT_PASS=adminadmin

# API keys (can be auto-fetched after first run if left empty)
SONARR_API_KEY=
RADARR_API_KEY=
PROWLARR_API_KEY=

# Rclone
RCLONE_REMOTE=mygdrive:           # or your chosen remote
RCLONE_MOUNT_PATH=/media/gdrive  # container mount path

# Timezone
TZ=Europe/Oslo
